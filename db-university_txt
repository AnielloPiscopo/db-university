-------------------------------------------------------------------------EX.1--------------------------------------------------------------------

SELECT students.* FROM students WHERE YEAR(date_of_birth) = 1990; 



-------------------------------------------------------------------------EX.2--------------------------------------------------------------------
SELECT courses.* FROM courses WHERE cfu >= 10; 



-------------------------------------------------------------------------EX.3--------------------------------------------------------------------
SELECT students.* FROM students WHERE YEAR(CURDATE()) - YEAR(date_of_birth) = 30; 



-------------------------------------------------------------------------EX.4--------------------------------------------------------------------
SELECT courses.* FROM courses WHERE (courses.year AND courses.period) = 1; 



-------------------------------------------------------------------------EX.5--------------------------------------------------------------------
SELECT exams.* FROM exams WHERE date = 20/06/2020 AND hour >= 14; 



-------------------------------------------------------------------------EX.6--------------------------------------------------------------------
SELECT degrees.* FROM degrees WHERE level = "master's degree"; 



-------------------------------------------------------------------------EX.7--------------------------------------------------------------------
SELECT COUNT('departments.*') FROM departments; 



-------------------------------------------------------------------------EX.8--------------------------------------------------------------------
SELECT COUNT('teachers.*') FROM teachers WHERE phone IS NULL; 